<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>SearchRaoVatController</servlet-name>
        <servlet-class>org.me.ViSearchController.SearchRaoVatController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetailRaoVatController</servlet-name>
        <servlet-class>org.me.ViSearchController.DetailRaoVatController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchWikiController</servlet-name>
        <servlet-class>org.me.ViSearchController.SearchWikiController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MemberLoginController</servlet-name>
        <servlet-class>org.me.ViSearchController.MemberLoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>jcaptcha</servlet-name>
        <servlet-class>com.octo.captcha.module.servlet.image.SimpleImageCaptchaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterMemberController</servlet-name>
        <servlet-class>org.me.ViSearchController.RegisterMemberController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>jcaptcha</servlet-name>
        <url-pattern>/jcaptcha.jpg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailRaoVatController</servlet-name>
        <url-pattern>/DetailRaoVatController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchRaoVatController</servlet-name>
        <url-pattern>/SearchRaoVatController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchWikiController</servlet-name>
        <url-pattern>/SearchWikiController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MemberLoginController</servlet-name>
        <url-pattern>/MemberLoginController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterMemberController</servlet-name>
        <url-pattern>/RegisterMemberController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
